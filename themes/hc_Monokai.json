{
	"$schema": "vscode://schemas/color-theme",
	"name": "Monokai High Contrast",
	"type": "hc",
	"colors": {
		"editor.background": "#000000",
		"editor.foreground": "#FFFFFF",
		"editorIndentGuide.background": "#FFFFFF",
		"sideBarTitle.foreground": "#FFFFFF"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#FFFFFF",
				"background": "#000000"
			}
		},

		// General
		{
			"scope": "comment",
			"settings": {
				"foreground": "#B3B3B3"
			}
		},
		{
			"name": "Types declaration and reference",
			"scope": [
				"meta.return-type",
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.class",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
			],
			"settings": {
				"fontStyle": [
					"italic",
					"underline"
				],
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.return.type",
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.other.inherited-class"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Function identifier",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars"
			],
			"settings": {
				"foreground": "#a6e22e"
			}
		},
		{
			"name": "Function call",
			"scope": [
				"meta.function-call"
			],
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Arguments",
			"scope": [
				"punctuation.section.arguments"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#a6e22e"
			}
		},
		{
			"name": "Parameters",
			"scope": [
				"punctuation.section.parameters"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#a6e22e"
			}
		},
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"name": "Core punctuation",
			"scope": [
				"punctuation.separator",
				"punctuation.terminator",
				"punctuation.other"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#F3C6BE"
				// "background": ""
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"name": "Control flow keywords",
			"scope": [
				"keyword.control",
				"keyword.operator",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.logical.python",
				"keyword.package",		// golang
				"keyword.import"		// golang
			],
			"settings": {
				"foreground": "#f92672"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#ae81ff"
			}
		},
		{
			"name": "Storage Type",
			"scope": [
				"storage.type"
			],
			"settings": {
				"foreground": "#66d9ef"
			}
		},
		{
			"scope": "storage.modifier",
			"settings": {
				"foreground": "#f92672"
			}
		},
		{
			"scope": [
				"string",
				"string.tag",
				"string.value"
			],
			"settings": {
				"foreground": "#f9ec6d"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#d16969"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant.character",
				"constant.other",
				"constant.numeric",
				"constant.language"
			],
			"settings": {
				"foreground": "#ae81ff"
			}
		},
		{
			"name": "TS this",
			"scope": "variable.language.this",
			"settings": {
				"fontStyle": "",
				"foreground": "#fc9b1b"
			}
		},

		// CSS
		{
			"name": "CSS property value",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#ce9178"
			}
		},

		// HTML and XML
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "#F92672"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Tag bracket",
			"scope": [
				"meta.tag.inline",
				"punctuation.definition.tag"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#5CDBEF"
			}
		},
		
		// Makefiles
		{
			"name": "Make prerequisites",
			"scope": "meta.scope.prerequisites.makefile",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#f9ec6d"
			}
		},
		{
			"name": "Make key value",
			"scope": "punctuation.separator.key-value.makefile",
			"settings": {
				"foreground": "#f92572"
			}
		},

		// Golang specific
		{
			"name": "Go construct types",
			"scope": [
				"keyword.struct.go",
				"keyword.map.go",
				"keyword.interface.go"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#66d9ef"
			}
		},
		{
			"name": "Golang Declaration Keywords",
			"scope": [
				"keyword.type.go",
				"keyword.function.go",
				"keyword.var.go"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#f92672"
			}
		}
	]
}
